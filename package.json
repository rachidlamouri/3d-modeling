{
  "scripts": {
    "build:jscadExample": "jscad build/src/jscadExample.js",
    "build:test": "nodemon --exec \"jscad build/src/test/thingBuild.js -o build/src/test.stl\"",
    "build:widgetExample": "jscad build/src/widgetExample.js",
    "clean": "rm -r build/",
    "compile:ts": "tsc",
    "compile:grammar": "rm -rf src/expressionParser/compiled/ && antlr4ts -visitor -o src/expressionParser/compiled/ src/expressionParser/DimensionScript.g4",
    "lint": "eslint --ext .js,.ts .",
    "postinstall": "npm run compile:grammar",
    "test": "mocha build/src/tests/**/*.test.js"
  },
  "dependencies": {
    "@jscad/cli": "^2.2.5",
    "@jscad/modeling": "^2.5.0",
    "antlr4ts": "^0.5.0-alpha.4",
    "jpeg-js": "^0.4.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/chai": "^4.2.22",
    "@types/lodash": "^4.14.173",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.2",
    "@types/sinon": "^10.0.4",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.24.0",
    "mocha": "^9.1.2",
    "nodemon": "^2.0.13",
    "sinon": "^11.1.2",
    "typescript": "^4.3.5"
  }
}
